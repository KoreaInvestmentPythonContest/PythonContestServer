INSERT INTO myDB.NEWS(OCCR_DT,TITLE,OCCR_LOC,URL,TEXT,IMG_URL_LIST) VALUES ('20221004','"이지스자산운용, 증권부문에 장지영 신임 대표 선임">이지스자산운용, 증권부문에 장지영 신임 대표 선임','네이버금융_뉴스포커스_채권선물','https://finance.naver.com/news/news_read.naver?article_id=0013480971&office_id=001&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=404&date=20221004&page=1','장지영 증권부문 신임 대표[이지스자산운용 제공. 재판매 및 DB 금지]    (서울=연합뉴스) 홍유담 기자 = 이지스자산운용은 지난 1일 주식, 채권 등 전통 자산에 투자하는 증권부문을 총괄하는 자리에 장지영 신임 부문대표를 선임했다고 4일 밝혔다.    장 신임 대표는 1998년 BNK투자증권에 입사해 2004년에 우정사업본부 자금운용팀, 2007년 국민연금 해외증권실 해외채권팀을 거쳤고, 2016년 한화자산운용에 합류해 채권본부장을 지내기도 했다.    이지스자산운용은 지난 4월 증권부문을 신설했다. 이지스자산운용은 종합자산운용사로 발돋움하기 위해 올해 부동산, 인프라 등 대체자산 중심의 사업 영역을 주식, 채권 등으로 확대하고 있다고 설명했다.     장 신임 대표는 "이지스자산운용이 보유한 대체투자의 경험과 노하우를 전통 자산과 연결하고, 리스크를 분산한 안정적인 수익의 다양한 상품을 투자자에게 선보이겠다"고 말했다.    ydhong@yna.co.kr','https://imgnews.pstatic.net/image/001/2022/10/04/AKR20221004114400002_01_i_P4_20221004144417681.jpg?type=w540');
[{'OCCR_DT': '20221004', 'TITLE': '"압타머사이언스, 165억 규모 전환사채권 발행 결정">압타머사이언스, 165억 규모 전환사채권 발행 결정', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333261&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 압타머사이언스(291650)는 운영자금 확보 목적으로 주식회사 BNK투자증권 등 대상 165억원 규모의 1회사 무기명식 이권부 무보증 사모 전환사채 발행을 결정했다고 4일 공시했다. 사채의 표면, 만기 이자율은 각각 0%. 0%이며, 만기일은 2027년 10월6일이다.전환가액은 8116원이며, 전환에 따라 발행할 주식수는 203만3021주로 주식총수대비 19.32% 규모다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"거래소, SK6호스팩 상장폐지 사유 발생">거래소, SK6호스팩 상장폐지 사유 발생', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333259&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 한국거래소 코스닥시장본부는 4일 SK6호스팩(340350)에 대해 상장폐지 사유가 발생해 5일 주권매매거래가 정지된다고 밝혔다. 상장폐지 사유는 상장예비심사 청구서 미제출로 관리종목 지정 후 1개월 이내 사유 미해소다.정리매매 기간은 오는 6일부터 17일까지다. 상장폐지일은 이달 18일이다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"카나리아바이오, 300억원 규모 제3자배정 유상증자 결정">카나리아바이오, 300억원 규모 제3자배정 유상증자 결정', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333256&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 카나리아바이오(016790)는 채무상환자금 약 300억원을 조달하기 위해 제3자 배정 유상증자를 결정했다고 4일 공시했다. 주당 2만581원에 신주 145만7655주가 발행된다. 제3자 배정 대상자는 세종메디칼이다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"에이치앤비디자인, 80억 규모 전환사채권 발행 결정">에이치앤비디자인, 80억 규모 전환사채권 발행 결정', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333250&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 에이치앤비디자인(227100)은 운영자금 확보 목적으로 주식회사 레이라인 등 대상 80억원 규모의 8회사 무기명식 이권부 무보증 사모 전환사채 발행을 결정했다고 4일 공시했다. 사채의 표면, 만기 이자율은 각각 2%. 4%이다. 만기일은 2025년 12월21일이다.전환가액은 5084원이다. 전환에 따라 발행할 주식수는 157만3564주로 주식총수 대비 14.29% 규모다. 전환청구 기간은 2023년 12월21일부터 2025년 11월20일까지다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"거래소, 인트로메딕 불성실공시법인 지정">거래소, 인트로메딕 불성실공시법인 지정', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333247&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 한국거래소 코스닥시장본부는 4일 인트로메딕(150840)에 대해 전환사채권 발행결정 철회 공시번복에 따라 불성실공시법인으로 지정한다고 밝혔다.지정일은 5일, 부과벌점은 5점이다. 인트로메딕은 최근 1년간 불성실공시법인 부과벌점이 5점이다. 현재 매매거래 정지 중으로 불성실공시법인 지정(부과벌점 8.0점 이상)에 따른 별도의 매매거래정지는 없다고 거래소는 설명했다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"파인테크닉스, 7일 주권매매거래 정지 해제">파인테크닉스, 7일 주권매매거래 정지 해제', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333245&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 한국거래소 코스닥시장본부는 4일 파인테크닉스(106240)에 대해 오는 7일 주권매매거래 정지를 해제한다고 밝혔다. 거래소는 “매매거래 재개일의 장개시 전 시간외매매는 성립되지 않는다”고 했다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"거래소, MDS테크 주식분할에 7일부터 주권매매 거래 정지">거래소, MDS테크 주식분할에 7일부터 주권매매 거래 정지', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333243&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 한국거래소 코스닥시장본부는 4일 MDS테크에 대해 오는 7일부터 신주권 변경상장일 전일까지 주권매매 거래를 정지한다고 밝혔다.이는 주식의 병합, 분할 등 전자등록 변경, 말소에 따른 것이다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '"서울제약, 상장적격성 실질심사 사유 발생">서울제약, 상장적격성 실질심사 사유 발생', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333242&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 한국거래소 코스닥시장본부는 4일 서울제약(018680)에 대해 회계처리 위반으로 인한 상장적격성 실질심사 사유가 발생했다고 밝혔다. 이에 따라 이달 5일부터  상장적격성 실질심사 대상여부에 관한 결정일까지 주권매매 거래가 정지된다.이날 서울제약은 2016년부터 2020년까지 매출 및 원가 허위계상, 외부감사 방해한 사실이 드러났다. 증권선물위원회는 감사인지정 3년 조치를 비롯해 회사와 전 대표이사 2인, 전 임원2인, 전 담당임원을 검찰에 고발했다. 회사 측은 “관련 기관과 협의해 적절한 조치를 취할 예정”이라며 “회계투명성 제고 및 내부회계관리제도를 강화했다. 차후 동일한 상황이 발생하지 않도록 회계처리 기준을 준수하겠다”고 밝혔다.', 'IMG_URL_LIST': ''}, {'OCCR_DT': '20221004', 'TITLE': '\'남양유업 "홍원식 회장, 주식양도 소송판결 불복해 항소"\'>남양유업 "홍원식 회장, 주식양도 소송판결 불복해 항소"', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0013481849&office_id=001&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '남양유업[남양유업 제공. 재판매 및 DB 금지]    (서울=연합뉴스) 신선미 기자 = 남양유업은 홍원식 회장 측이 주식양도 소송 판결에 불복하며 항소를 제기했다고 4일 공시했다.    사모펀드 운용사 한앤컴퍼니는 지난해 5월 홍 회장 일가가 보유한 남양유업 지분을 인수하는 주식매매계약(SPA)을 맺었으나 홍 회장 측은 같은 해 9월 계약 해지를 통보했다.    이에 한앤컴퍼니는 계약대로 주식을 양도하라며 홍원식 회장 일가를 상대로 소송을 냈고, 서울중앙지법 민사합의30부(정찬우 부장판사)는 지난달 22일 한앤컴퍼니의 손을 들어줬다.    홍 회장 일가가 앞선 계약대로 주식을 이전하는 절차를 이행해야 한다는 것이 판결의 요지였다.    그러나 홍 회장 측은 이번 판결에 불복하면서 이날 항소를 제기했다.    홍 회장 측 대리인은 지난 달 판결 이후에도 "한앤컴퍼니 측의 쌍방대리 행위로 권리를 제대로 보호받지 못했다"며 "이런 내용을 재판부가 충분히 받아들이지 않은 것 같아 유감스럽게 생각하며 즉시 항소할 계획"이라고 밝혔다.    sun@yna.co.kr', 'IMG_URL_LIST': 'https://imgnews.pstatic.net/image/001/2022/10/04/PCM20210707000025030_P4_20221004190516783.jpg?type=w540'}, {'OCCR_DT': '20221004', 'TITLE': '\'지더블유바이텍 "법원서 임시 주총 소집 허가"\'>지더블유바이텍 "법원서 임시 주총 소집 허가"', 'OCCR_LOC': '네이버금융_뉴스포커스_공시메모', 'URL': 'https://finance.naver.com/news/news_read.naver?article_id=0005333236&office_id=018&mode=LSS3D&type=0&section_id=101&section_id2=258&section_id3=406&date=20221004&page=1', 'TEXT': '[이데일리 양지윤 기자] 지더블유바이텍(036180)은 수본생활건강이 제기한 임시 주주총회 소집을 서울남부지방법원에서 허가했다고 4일 공시했다.  법원은 조완규 수본생활건강의 대표이사를 임시 주총의장으로 한다는 청구내용도 받아들였다.회사 측은 “법적 절차에 따라 적극 대응할 예정”이라고 밝혔다.', 'IMG_URL_LIST': ''}]
